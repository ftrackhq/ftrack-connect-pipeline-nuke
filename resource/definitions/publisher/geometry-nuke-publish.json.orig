{
  "type": "publisher",
  "name": "Geometry Publisher",
  "asset_type": "geo",
  "host_type": "nuke",
  "ui_type": "qt",
  "contexts": [
    {
      "name": "main",
      "stages": [
        {
          "name": "context",
          "plugins":[
            {
              "name": "context selector",
              "plugin": "common_default_publisher_context",
              "widget": "common_default_publisher_context"
            }
          ]
        }
      ]
    }
  ],
  "components": [
    {
      "name": "snapshot",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "collect scene",
              "plugin": "nuke_scene_publisher_collector",
              "options": {
                "export": ["scene"]
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "Check file saved",
              "plugin": "nuke_file_saved_publisher_validator"
            },
            {
              "name": "validate file exists",
              "plugin": "nuke_file_exists_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "write nuke scene",
              "plugin": "nuke_default_publisher_exporter"
            }
          ]
        }
      ]
    },
    {
      "name": "cache",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Selection collector",
              "plugin": "nuke_default_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "no validator",
              "plugin": "common_test_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "Export geometry",
              "plugin": "nuke_geometry_publisher_exporter",
              "options": {
                  "file_type": "abc",
                  "animation" : true,
                  "start" : "",
                  "end" : ""
              }
            }
          ]
        }
      ]
    },
    {
      "name": "game",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Selection collector",
              "plugin": "nuke_default_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "no validator",
              "plugin": "common_test_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "Export geometry",
              "plugin": "nuke_geometry_publisher_exporter",
              "options": {
                  "file_type": "fbx"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "geo",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Selection collector",
              "plugin": "nuke_default_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "no validator",
              "plugin": "common_test_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "Export geometry",
              "plugin": "nuke_geometry_publisher_exporter",
              "options": {
                  "file_type": "obj"
              }
            }
          ]
        }
      ]
    }
    ],
  "finalizers": [
    {
      "name": "main",
      "stages": [
        {
          "name": "pre_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "pre to ftrack server",
              "plugin": "common_default_publisher_pre_finalizer"
            }
          ]
        },
        {
          "name": "finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "to ftrack server",
              "plugin": "nuke_default_publisher_finalizer"
            }
          ]
        },
        {
          "name": "post_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "post to ftrack server",
              "plugin": "common_default_publisher_post_finalizer"
            }
          ]
        }
      ]
    }
  ]
}