{
  "type": "publisher",
  "name": "Movie Publisher",
  "asset_type": "render",
  "host_type": "nuke",
  "ui_type": "qt",
  "contexts": [
    {
      "name": "main",
      "stages": [
        {
          "name": "context",
          "plugins":[
            {
              "name": "context selector",
              "plugin": "common_default_publisher_context",
              "widget": "common_default_publisher_context"
            }
          ]
        }
      ]
    }
  ],
  "components": [
    {
      "name": "snapshot",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "collect scene",
              "plugin": "nuke_scene_publisher_collector",
              "options": {
                "export": ["scene"]
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "Check file saved",
              "plugin": "nuke_file_saved_publisher_validator"
            },
            {
              "name": "validate file exists",
              "plugin": "nuke_file_exists_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "write nuke scene",
              "plugin": "nuke_default_publisher_exporter"
            }
          ]
        }
      ]
    },
    {
      "name": "movie",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "collect movie/sequence node.",
              "plugin": "nuke_default_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "common_non_empty_publisher_validator"
            },
            {
              "name": "Scene saved",
              "plugin": "nuke_file_saved_publisher_validator"
            },
            {
              "name": "Can connect to write node",
              "plugin": "nuke_writable_node_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "write movie",
              "plugin": "nuke_movie_publisher_exporter",
              "options": {
                "mode": "render",
                "file_format": "mov",
                "mov": {
                  "mov64_codec": "mp4v"
                }
              }
            }
          ]
        }
      ]
    },
    {
      "name": "thumbnail",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "collect write node.",
              "plugin": "nuke_default_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "nuke_writable_node_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "write thumbnail",
              "plugin": "nuke_thumbnail_publisher_exporter"
            }
          ]
        }
      ]
    },
    {
      "name": "reviewable",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "collect sequence node.",
              "plugin": "nuke_default_publisher_collector"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "common_non_empty_publisher_validator"
            },
            {
              "name": "Can connect to write node",
              "plugin": "nuke_writable_node_publisher_validator"
            }
          ]
        },
        {
          "name": "exporter",
          "plugins":[
            {
              "name": "write reviewable",
              "plugin": "nuke_reviewable_publisher_exporter",
              "options": {
                  "mode": "render"
              }
            }
          ]
        }
      ]
    }
  ],
  "finalizers": [
    {
      "name": "main",
      "stages": [
        {
          "name": "pre_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "pre to ftrack server",
              "plugin": "common_default_publisher_pre_finalizer"
            }
          ]
        },
        {
          "name": "finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "to ftrack server",
              "plugin": "nuke_default_publisher_finalizer"
            }
          ]
        },
        {
          "name": "post_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "post to ftrack server",
              "plugin": "common_default_publisher_post_finalizer"
            }
          ]
        }
      ]
    }
  ]
}